config_version=5

[application]

config/name="1BitTowerjam"
run/main_scene="res://world.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://Textures/icon.svg"

[audio]

buses/default_bus_layout="res://Audio/AudioBus.tres"

[autoload]

Util="*res://Util.gd"
Enemy="*res://Enemy.gd"
AudioController="*res://audio_controller.tscn"
AStarUtil="*res://astar.gd"

[display]

window/size/viewport_width=672
window/size/viewport_height=416
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://Prefabs/": "orange",
"res://Shaders/": "blue",
"res://Textures/": "green"
}

